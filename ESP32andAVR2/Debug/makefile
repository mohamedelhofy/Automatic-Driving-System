-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
LSS += \
ESP32andAVR2.lss \

FLASH_IMAGE += \
ESP32andAVR2.hex \

SIZEDUMMY += \
sizedummy \


# All Target
all: ESP32andAVR2.elf secondary-outputs

# Tool invocations
ESP32andAVR2.elf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: AVR C Linker'
	avr-gcc -Wl,-Map,ESP32andAVR2.map -mmcu=atmega32 -o "ESP32andAVR2.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

ESP32andAVR2.lss: ESP32andAVR2.elf
	@echo 'Invoking: AVR Create Extended Listing'
	-avr-objdump -h -S ESP32andAVR2.elf  >"ESP32andAVR2.lss"
	@echo 'Finished building: $@'
	@echo ' '

ESP32andAVR2.hex: ESP32andAVR2.elf
	@echo 'Create Flash image (ihex format)'
	-avr-objcopy -R .eeprom -R .fuse -R .lock -R .signature -O ihex ESP32andAVR2.elf  "ESP32andAVR2.hex"
	@echo 'Finished building: $@'
	@echo ' '

sizedummy: ESP32andAVR2.elf
	@echo 'Invoking: Print Size'
	-avr-size --format=avr --mcu=atmega32 ESP32andAVR2.elf
	@echo 'Finished building: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) $(FLASH_IMAGE)$(ELFS)$(OBJS)$(ASM_DEPS)$(S_DEPS)$(SIZEDUMMY)$(S_UPPER_DEPS)$(LSS)$(C_DEPS) ESP32andAVR2.elf
	-@echo ' '

secondary-outputs: $(LSS) $(FLASH_IMAGE) $(SIZEDUMMY)

.PHONY: all clean dependents
.SECONDARY:

-include ../makefile.targets
